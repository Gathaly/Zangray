function sort(arr, len):
	cnt = 0
	loop cnt < len:		
		index = 0		
		loop index < len - cnt - 1 :			
		  if arr[index] > arr[index+1]:				
			temp = arr[index+1]				
			arr[index+1] = arr[index]				
			arr[index] = temp
		  ;			
		  index++
		;	
		cnt++
	;
;
...自定义测试数组并冒泡排序
arr = [3 , 1230 , 55555 , 6 , 7, 6, 10]
sort(arr, arr.len)
